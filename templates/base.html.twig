<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        {# Run `composer require symfony/webpack-encore-bundle`
           and uncomment the following Encore helpers to start using Symfony UX #}
        <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
        <link href="https://fonts.googleapis.com/css2?family=GFS+Didot&family=Lato:wght@100;300;400&display=swap" rel="stylesheet">
        <link rel="stylesheet" href="/build/app.css">
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}
        <link rel="icon" href="{{ asset('build/favicon.png') }}" alt="favicon">
    </head>
    <body>
        <header>{% include '_navbar.html.twig' %}</header>
        <div class="content">{% block content %}{% endblock %}</div>
        <div class="footer">{% block footer %}{% include '_footer.html.twig' %}{% endblock %}</div>

        <script src="/build/app.js"></script>
        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}
    </body>
</html>
